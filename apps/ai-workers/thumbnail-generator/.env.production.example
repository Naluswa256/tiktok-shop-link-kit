# Production Environment Configuration for Thumbnail Generator
NODE_ENV=production

# AWS Configuration (will be set by deployment script)
AWS_REGION=us-east-1

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/your-queue-name
BATCH_SIZE=1
MAX_RETRIES=3
VISIBILITY_TIMEOUT=900
WAIT_TIME_SECONDS=20

# SNS Configuration
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:your-topic-name

# S3 Configuration
S3_BUCKET_NAME=your-s3-bucket-name

# Video Processing Settings
MAX_VIDEO_SIZE_MB=50
MAX_VIDEO_DURATION_SECONDS=60
FRAME_EXTRACTION_INTERVAL=2
MAX_FRAMES_TO_ANALYZE=15
THUMBNAILS_TO_GENERATE=5

# YOLO Settings
YOLO_SERVICE_URL=http://localhost:8000
YOLO_SERVICE_PORT=8000
YOLO_MODEL_PATH=yolov8n.pt
YOLO_CONFIDENCE_THRESHOLD=0.5
YOLO_IOU_THRESHOLD=0.5

# Quality Thresholds
MIN_QUALITY_SCORE=0.4
MIN_BRIGHTNESS_SCORE=0.3
MAX_BLUR_SCORE=0.7

# Output Settings
THUMBNAIL_WIDTH=400
THUMBNAIL_HEIGHT=400
THUMBNAIL_QUALITY=85

# Logging
LOG_LEVEL=info

# Health Check
ENABLE_HEALTH_CHECK=true
HEALTH_CHECK_PORT=8080
