# Production Environment Configuration for Auto Tagger
NODE_ENV=production

# AWS Configuration (will be set by deployment script)
AWS_REGION=us-east-1

# SQS Configuration
SQS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/your-queue-name
BATCH_SIZE=10
MAX_RETRIES=3
VISIBILITY_TIMEOUT=900
WAIT_TIME_SECONDS=20

# SNS Configuration
SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:your-topic-name

# LLM Configuration (OpenRouter)
LLM_PROVIDER=openrouter
LLM_MODEL=microsoft/phi-3-mini-128k-instruct:free
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Tagging Configuration
MAX_TAGS_PER_PRODUCT=10
MIN_TAG_CONFIDENCE=0.6
ENABLE_CATEGORY_DETECTION=true
ENABLE_BRAND_DETECTION=true
ENABLE_ATTRIBUTE_DETECTION=true

# Logging
LOG_LEVEL=info

# Health Check
ENABLE_HEALTH_CHECK=true
HEALTH_CHECK_PORT=8080
